# Clear Previous Runs
reset_design

# -------------------------------------------------
# Clock Generation
# -------------------------------------------------
create_clock -name "clk" -period 0.8 [get_ports clk]
Input delay     0.1                  [all_clocks]
output delay    0.1                  [all_clocks]

# # Values to Consider: Below
set_clock_uncertainty   0.004         [all_clocks] 
set_clock_transition    0.003         [all_clocks]
